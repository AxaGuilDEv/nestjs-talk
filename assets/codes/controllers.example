import {
  Get,
  Put,
  Body,
  Controller,
  Param,
} from '@nestjs/common';

@Controller('call-for-papers')
export class CFPController {

  @Get(':id')
  find(@Param('id') id: string): string {
    return `This action returns a CFP #${id}`;
  }

  @Put(':id')
  update(@Param('id') id, @Body() updateCFPDto) {
    return `This action updates the CFP ${updateCFPDto.url} #${id}`;
  }
}